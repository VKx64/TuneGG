# Environment Variables Template for TuneGG
# Copy this file to .env and fill in your actual values
# DO NOT commit .env files to version control

# PocketBase Configuration
# The URL to your PocketBase server instance
# For local development, use: http://127.0.0.1:8090
# For production, use your actual PocketBase server URL
EXPO_PUBLIC_POCKETBASE_URL=https://your-pocketbase-server.com

# Development Configuration
# Uncomment the line below for local development
# EXPO_PUBLIC_POCKETBASE_URL=http://127.0.0.1:8090

# Additional configuration (add as needed)
# EXPO_PUBLIC_API_TIMEOUT=5000
# EXPO_PUBLIC_DEBUG_MODE=false